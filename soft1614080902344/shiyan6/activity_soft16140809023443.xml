<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.hzuapps.androidlabs.soft1614080902344.Soft1614080902344Activity3">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="请输入你想登录学生管理系统界面的网址："
        android:id="@+id/textView11" />

        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="0dp"
        android:orientation="horizontal">
        <EditText
            android:id="@+id/imagepath"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="http://img.redocn.com/shejigao/20120710/20120708_8614cff8b6b14befd3a1841YcMhts0tN.jpg"
            android:layout_below="@+id/textView11"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="下载"
        android:id="@+id/download"
        android:layout_below="@+id/imagepath"
        android:layout_toLeftOf="@+id/fanhui"
        android:layout_toStartOf="@+id/fanhui"
        android:layout_marginRight="39dp"
        android:layout_marginEnd="39dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="返回"
        android:id="@+id/fanhui"
        android:layout_alignTop="@+id/download"
        android:layout_alignRight="@+id/textView11"
        android:layout_alignEnd="@+id/textView11" />
   

</RelativeLayout>
